   22 nome_agente: ID

   23 stmt: ε
   24     | PLANOS '{' planos '}'
   25     | OBJETIVOS '{' objetivos '}'
   26     | nome_agente CRENCAS '{' crencas '}'

   27 list_stmt: ε
   28          | stmt EOL list_stmt
   29          | '0'


